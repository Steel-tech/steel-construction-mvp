# Frontend Environment Configuration
# Copy this file to .env.local and update with your values

# API Configuration
VITE_API_URL=http://localhost:5001/api/v1
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=steel_auth_token
VITE_AUTH_REFRESH_KEY=steel_refresh_token

# Supabase Configuration (if using Supabase)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# External Services
VITE_SENTRY_DSN=
VITE_GOOGLE_MAPS_KEY=
VITE_CLOUDINARY_URL=

# Application Configuration
VITE_APP_NAME="Steel Construction MVP"
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LOCALE=en
VITE_DATE_FORMAT=MM/DD/YYYY

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# WebSocket Configuration (if needed)
VITE_WS_URL=ws://localhost:5001
VITE_WS_RECONNECT_INTERVAL=5000

# Cache Configuration
VITE_CACHE_TTL=3600000
VITE_USE_LOCAL_STORAGE=true

# Development Tools
VITE_SHOW_DEV_TOOLS=false
VITE_MOCK_API=false
