# Production Environment Configuration for Frontend
# Update these values with your production URLs

# API Configuration
VITE_API_URL=https://api.your-domain.com/api/v1
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=steel_auth_token
VITE_AUTH_REFRESH_KEY=steel_refresh_token

# Application Settings
VITE_APP_NAME="Steel Construction MVP"
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LOCALE=en
VITE_DATE_FORMAT=MM/DD/YYYY

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# External Services (Optional)
VITE_SENTRY_DSN=
VITE_GOOGLE_MAPS_KEY=
VITE_CLOUDINARY_URL=

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Cache Configuration
VITE_CACHE_TTL=3600000
VITE_USE_LOCAL_STORAGE=true

# Production Settings
VITE_SHOW_DEV_TOOLS=false
VITE_MOCK_API=true
